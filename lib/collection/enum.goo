;; enum.gooli -- enumerator

(use primitives)

(defclass <enum> ())

(def enum (x|<any>) (error "enum not implemented"))
(def fin? (e|<enum>) (error "FIN? not implemented"))
(def nxt (e|<enum>) (error "NXT not implemented"))
(def now (e|<enum>) (error "NOW not implemented"))
(def now-setter (v|<any> e|<enum>) (error "NOW-SETTER not implemented"))
(def now-key (e|<enum>) (error "NOW-KEY not implemented"))

(defmacro for (e) e)

(export <enum>
	enum fin? nxt now now-setter now-key)
