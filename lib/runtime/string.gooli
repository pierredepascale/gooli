;;; string.gooli -- string operations

(use primitives 
     runtime/char)

(def str (&rest ch) "<not-implemented>")

(def case-insensitive-string-hash (str|<string>)
  123)

(def case-insensitive-string-equal (s1|<string> s2|<string>)
  123)

(def to-lower (s|<string>) (map to-lower s))

(def to-upper (s|<string>) (map to-upper s))

(export str case-insensitive-string-hash
	case-insensitive-string-equal)
