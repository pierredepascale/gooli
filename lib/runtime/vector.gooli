;;; vector.gooli -- vector procedure

(def elt (v|<vector> i) (%vector-ref v i))
(def elt-setter (v|<vector> i o) (%vector-set! v i o))

(def map (proc|<fun> v|<vector>) v)
(def fold-left (proc|<fun> init|<any> v|<vector>)
  (as <vector> (fold-left proc init (as <list> v))))
(def fold-right (proc|<fun> init|<any> v|<vector>)
  (as <vector> (file-right proc init (as <list> v))))

(def as (type|(t= <vector>) l|<list>)
  (#()))

(def as (type|(t= <list>) v|<vector>)
  '())
